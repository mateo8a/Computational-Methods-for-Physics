Cell["Problem 6.14", "Subsection",
 CellChangeTimes->{{3.6229175791012106`*^9, 3.6229175833714547`*^9}}],

Cell["\<\
Implementing our integrator to calculate the perimeter of an ellipse, we have:\
\>", "Text",
 CellChangeTimes->{{3.622919494277752*^9, 3.622919526979623*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"EllipsePL", "[", 
   RowBox[{"a_", ",", "b_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"e", ",", "c", ",", "Circ"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"e", "=", 
      RowBox[{"Sqrt", "[", 
       RowBox[{"1", "-", 
        RowBox[{
         RowBox[{"b", "^", "2"}], "/", 
         RowBox[{"a", "^", "2"}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"c", "[", "\[Theta]_", "]"}], ":=", 
      RowBox[{"4", "a", " ", 
       RowBox[{"Sqrt", "[", 
        RowBox[{"1", "-", 
         RowBox[{
          RowBox[{"e", "^", "2"}], " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"Sin", "[", "\[Theta]", "]"}], ")"}], "^", "2"}]}]}], 
        "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Circ", "=", 
      RowBox[{"LagrangeIF", "[", 
       RowBox[{"c", ",", "0", ",", 
        RowBox[{"Pi", " ", "/", "2"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "Circ", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.6229188871120243`*^9, 3.6229189050110483`*^9}, {
   3.622918972377901*^9, 3.6229189827084923`*^9}, {3.6229190227517824`*^9, 
   3.622919212513636*^9}, {3.6229193098222017`*^9, 3.622919313018385*^9}, {
   3.6229193652043695`*^9, 3.6229194019234695`*^9}, 3.6229195489848814`*^9}],

Cell["And using the Simpson method we have:", "Text",
 CellChangeTimes->{{3.6229195543051853`*^9, 3.622919564895791*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"EllipsePS", "[", 
   RowBox[{"a_", ",", "b_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"e", ",", "c", ",", "Circ"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"e", "=", 
      RowBox[{"Sqrt", "[", 
       RowBox[{"1", "-", 
        RowBox[{
         RowBox[{"b", "^", "2"}], "/", 
         RowBox[{"a", "^", "2"}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"c", "[", "\[Theta]_", "]"}], ":=", 
      RowBox[{"4", "a", " ", 
       RowBox[{"Sqrt", "[", 
        RowBox[{"1", "-", 
         RowBox[{
          RowBox[{"e", "^", "2"}], " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"Sin", "[", "\[Theta]", "]"}], ")"}], "^", "2"}]}]}], 
        "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Circ", "=", 
      RowBox[{"Simpson", "[", 
       RowBox[{"c", ",", "0", ",", 
        RowBox[{"Pi", " ", "/", "2"}], ",", "4"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "Circ", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.6229195674589376`*^9, 3.6229195974356527`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Inum", "=", 
   RowBox[{"EllipsePL", "[", 
