Cell["\<\
The following function is based off the \[OpenCurlyDoubleQuote]Lagrange\
\[CloseCurlyQuote]s interpolating formula\[CloseCurlyDoubleQuote], with n=3.\
\>", "Text",
 CellChangeTimes->{{3.622915670647053*^9, 3.6229157012068005`*^9}, {
  3.6229157550348797`*^9, 3.622915814570285*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"LagrangeIF", "[", 
   RowBox[{"f_", ",", "xi_", ",", "xf_", ",", "Ns_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "w0", ",", "w1", ",", "w2", ",", "w3", ",", "dx", ",", "J", ",", "i"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"dx", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"xf", "-", "xi"}], ")"}], "/", "Ns"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"w0", "=", 
      RowBox[{"3", "/", "8"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"w1", "=", 
      RowBox[{"9", "/", "8"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"w2", "=", 
      RowBox[{"9", "/", "8"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"w3", "=", 
      RowBox[{"3", "/", "8"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"J", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "0"}], ",", 
       RowBox[{"i", "\[LessEqual]", 
        RowBox[{"Ns", "-", "3"}]}], ",", 
       RowBox[{"i", "=", 
        RowBox[{"i", "+", "3"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"J", "=", 
        RowBox[{"J", "+", 
         RowBox[{"dx", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"f", "[", 
              RowBox[{"xi", "+", 
               RowBox[{"i", " ", "dx"}]}], "]"}], "w0"}], "+", 
            RowBox[{
             RowBox[{"f", "[", 
              RowBox[{"xi", "+", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"i", "+", "1"}], ")"}], " ", "dx"}]}], "]"}], "w1"}],
             "+", 
            RowBox[{
             RowBox[{"f", "[", 
              RowBox[{"xi", "+", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"i", "+", "2"}], ")"}], " ", "dx"}]}], "]"}], "w2"}],
             "+", 
            RowBox[{
             RowBox[{"f", "[", 
              RowBox[{"xi", "+", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"i", "+", "3"}], ")"}], "dx"}]}], "]"}], "w3"}]}], 
           ")"}]}]}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "J", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.6229148274238234`*^9, 3.6229148825879784`*^9}, {
  3.6229150117013636`*^9, 3.6229150233430295`*^9}, {3.6229150792592278`*^9, 
  3.622915160657883*^9}, {3.6229152061204834`*^9, 3.622915320486025*^9}, {
  3.622915350585747*^9, 3.62291535781116*^9}, {3.6236819122830105`*^9, 
  3.6236819335172253`*^9}, {3.623682005836362*^9, 3.6236821133185096`*^9}, {
  3.623682232939351*^9, 3.623682327984788*^9}}],

Cell["\<\
We\[CloseCurlyQuote]ll use the integrator above to integrate the following \
function:\
\>", "Text",
 CellChangeTimes->{{3.622915839352702*^9, 3.6229158784989414`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"x", "==", "0"}], ",", "1", ",", 
     RowBox[{
      RowBox[{"Sin", "[", "x", "]"}], "/", "x"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6229152918103848`*^9, 3.6229152939185057`*^9}, {
