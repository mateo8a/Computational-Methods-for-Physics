Cell["\<\
The next function will calculate the electric field when x=0 and y=0.\
\>", "Text",
 CellChangeTimes->{{3.6233570615930877`*^9, 3.623357095092004*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"ZField", "[", 
   RowBox[{"zi_", ",", "zf_", ",", "dz_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"field", ",", "z", ",", "i"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"field", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0.0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"zf", "-", "zi"}], ")"}], "+", 
           RowBox[{"1", "/", "dz"}]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "2"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"z", "=", "zi"}], ";", "\[IndentingNewLine]", 
     RowBox[{"i", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"z", "\[LessEqual]", "zf"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"field", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "1"}], "]"}], "]"}], "=", "z"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"field", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "2"}], "]"}], "]"}], "=", 
         RowBox[{"Ez", "[", 
          RowBox[{"0", ",", "0", ",", "z"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"z", "=", 
         RowBox[{"z", "+", "dz"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"i", "=", 
         RowBox[{"i", "+", "1"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "field", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.623351592486273*^9, 3.6233517582107515`*^9}, {
  3.62335182304946*^9, 3.6233518382853317`*^9}, {3.623351947723591*^9, 
