Cell[TextData[{
 "This function will give the number of \[OpenCurlyDoubleQuote]darts\
\[CloseCurlyDoubleQuote] needed to obtain the volume of the two dimensional \
sphere of radius ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " up to an error of approximately ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PlusMinus]", 
    FractionBox["di", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". The function will take a list with M approximations of the volume and \
take the difference between the higest and lowest values. It will start with \
one dart, and will increase the number of darts until the difference between \
the highest and lowest values is lower or equal to di. "
}], "Text",
 CellChangeTimes->{{3.623509688066924*^9, 3.6235097070040073`*^9}, {
  3.623509744667162*^9, 3.62351008433659*^9}, {3.623510127249044*^9, 
  3.623510147364195*^9}, {3.623510209966775*^9, 3.623510212229905*^9}, {
  3.623510375398237*^9, 3.6235104381858287`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Hits2D", "[", 
   RowBox[{"R_", ",", "M_", ",", "di_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"m", ",", "n", ",", "Idata"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Idata", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0.0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "M"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"m", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"n", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Idata", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "=", "20"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Idata", "[", 
       RowBox[{"[", "2", "]"}], "]"}], "=", "1"}], ";", "\[IndentingNewLine]",
      "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Max", "[", "Idata", "]"}], "-", 
         RowBox[{"Min", "[", "Idata", "]"}]}], ">", "di"}], ",", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{"m", "\[LessEqual]", "M"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"Idata", "[", 
             RowBox[{"[", "m", "]"}], "]"}], "=", 
            RowBox[{"MC2Dsph", "[", 
             RowBox[{"R", ",", "n"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"m", "=", 
            RowBox[{"m", "+", "1"}]}]}]}], "]"}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"n", "=", 
         RowBox[{"n", "+", "1"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"m", "=", "1"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "n", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.6233709762950735`*^9, 3.623371030510174*^9}, {
