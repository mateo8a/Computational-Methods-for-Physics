Cell["Problem 6.17", "Subsection",
 CellChangeTimes->{{3.6233571817679615`*^9, 3.623357190405455*^9}}],

Cell["\<\
This function will take a function and approximate it\[CloseCurlyQuote]s \
integral between a lower and an upper boundary for values of N up to a \
maximum N given by the user.\
\>", "Text",
 CellChangeTimes->{{3.6233676090394773`*^9, 3.6233676145947948`*^9}, {
  3.623367692331241*^9, 3.6233677108733015`*^9}, {3.6233678446009502`*^9, 
  3.6233678829231424`*^9}, {3.623367940261422*^9, 3.623367984826971*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"MonteCarlo", "[", 
   RowBox[{"f_", ",", "N_", ",", "lb_", ",", "ub_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"dx", ",", "n", ",", "m", ",", "x", ",", "I", ",", "Idata"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Idata", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0.0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "N"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"m", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"n", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"I", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"m", "\[LessEqual]", "N"}], ",", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{"n", "\[LessEqual]", " ", "m"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"dx", "=", 
            RowBox[{".5", "/", "m"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"x", "=", 
            RowBox[{"RandomReal", "[", 
             RowBox[{"{", 
              RowBox[{"lb", ",", "ub"}], "}"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"I", "=", 
            RowBox[{"I", "+", 
             RowBox[{
              RowBox[{"f", "[", "x", "]"}], " ", "dx"}]}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"n", "=", 
            RowBox[{"n", "+", "1"}]}]}]}], "]"}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Idata", "[", 
          RowBox[{"[", "m", "]"}], "]"}], "=", "I"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"m", "=", 
         RowBox[{"m", "+", "1"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"n", "=", "1"}], ";", "\[IndentingNewLine]", 
        RowBox[{"I", "=", "0"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "Idata", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
