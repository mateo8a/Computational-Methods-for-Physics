Cell["\<\
The approximation of the volume of a two dimensional sphere will be given by \
the next function.\
\>", "Text",
 CellChangeTimes->{{3.623509576985571*^9, 3.6235096449244566`*^9}, 
   3.623509680942517*^9, 3.6235103257473974`*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{"MC2Dsph", "[", 
   RowBox[{"R_", ",", "N_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", ",", "hit", ",", "x", ",", "y"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"hit", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"n", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"n", "\[LessEqual]", "N"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"x", "=", 
         RowBox[{"RandomReal", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", "R"}], ",", "R"}], "}"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"y", "=", 
         RowBox[{"RandomReal", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", "R"}], ",", "R"}], "}"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"x", "^", "2"}], "+", 
             RowBox[{"y", "^", "2"}]}], "]"}], "\[LessEqual]", "R"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"hit", "=", 
           RowBox[{"hit", "+", "1"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"n", "=", 
         RowBox[{"n", "+", "1"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"hit", "/", "N"}], ")"}], " ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"2", "R"}], ")"}], "^", "2"}]}], "]"}], ";"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.6233592720925207`*^9, 3.623359310098695*^9}, {
