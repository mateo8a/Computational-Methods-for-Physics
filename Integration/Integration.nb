Cell["Using Simpson\[CloseCurlyQuote]s method:", "Text",
 CellChangeTimes->{{3.6229158193515587`*^9, 3.622915825791927*^9}, {
  3.622915882285158*^9, 3.6229158901566086`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Simpson", "[", 
   RowBox[{"f_", ",", " ", "a_", ",", " ", "b_", ",", " ", "Ns_"}], "]"}], 
  " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"Ia", ",", " ", "j", ",", " ", "dx"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"dx", " ", "=", " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"b", "-", "a"}], ")"}], "/", "Ns"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Ia", " ", "=", " ", "0.0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"j", " ", "=", "0"}], ",", " ", 
       RowBox[{"j", " ", "\[LessEqual]", " ", 
        RowBox[{"Ns", "-", "2"}]}], ",", " ", 
       RowBox[{"j", " ", "=", " ", 
        RowBox[{"j", "+", "2"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Ia", " ", "=", " ", 
         RowBox[{"Ia", " ", "+", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "/", "3"}], ")"}], " ", "dx", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"f", "[", 
              RowBox[{"a", " ", "+", " ", 
               RowBox[{"j", " ", "dx"}]}], "]"}], " ", "+", " ", 
             RowBox[{"4", " ", 
              RowBox[{"f", "[", 
               RowBox[{"a", " ", "+", " ", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"j", "+", "1"}], ")"}], " ", "dx"}]}], "]"}]}], " ",
              "+", " ", 
             RowBox[{"f", "[", 
              RowBox[{"a", " ", "+", " ", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"j", "+", "2"}], ")"}], " ", "dx"}]}], "]"}]}], 
            ")"}]}]}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "Ia", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.5167333586349916`*^9, 3.516733438425005*^9}}],

